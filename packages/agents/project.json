{
  "name": "agents",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/agents/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "commands": ["rm -rf dist", "rollup -c rollup.config.js"],
        "cwd": "packages/agents",
        "parallel": false
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["pnpm publish"],
        "cwd": "packages/agents",
        "parallel": false
      }
    },
    "cli": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["pnpm ts-node packages/agents/scripts/cli.ts"]
      }
    }
  }
}
